{
  "name": "@fische/react",
  "version": "0.0.1",
  "description": "React Hooks for utility",
  "repository": "https://github.com/XunderbarX/fische",
  "main": "./dist/cjs/index.js",
  "types": "./dist/cjs/index.d.ts",
  "keywords": [
    "react",
    "custom hooks",
    "hooks",
    "utility",
    "util",
    "clipboard",
    "media query"
  ],
  "scripts": {
    "test-build": "vitest",
    "test": "vitest",
    "build": "bun run build-package .",
    "build-package": "bunchee --cwd",
    "release": "npm publish"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/es/index.d.mts",
        "default": "./dist/es/index.mjs"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    }
  },
  "files": [
    "./dist/**/*.{js,d.ts,mjs,d.mts}",
    "./README.md",
    "./README.*.md",
    "./package.json"
  ],
  "author": "vvyre <seung.yoon.yu@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@frfla/laser": "^0.1.0",
    "@swc/cli": "^0.3.12",
    "@swc/core": "^1.4.12",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^14.2.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/add": "^2",
    "@types/jsdom": "^27",
    "@types/node": "^20.12.4",
    "@types/react": "^18.2.74",
    "@types/react-dom": "^18.3.0",
    "@vitest/coverage-v8": "^4.0.9",
    "add": "^2.0.6",
    "axios": "^1.6.8",
    "bunchee": "^5.1.2",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-react": "7.34.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-testing-library": "6.2.0",
    "jsdom": "^27.2.0",
    "nock": "^13.5.4",
    "prettier": "^3.2.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.9"
  },
  "peerDependencies": {
    "react": ">=18"
  },
  "prettier": {
    "tabWidth": 2,
    "printWidth": 100,
    "semi": false,
    "useTabs": false,
    "singleQuote": true,
    "arrowParens": "avoid",
    "trailingComma": "none"
  },
  "packageManager": "yarn@4.11.0"
}
